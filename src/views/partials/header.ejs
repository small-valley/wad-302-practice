<header>
	<div id="header">
		<nav>
			<ul id="header-list">
				<button class="button-header">
					<li class="link"><a href="/app/urls">Home</a></li>
				</button>
				<button class="button-header">
					<li class="link"><a href="/auth/login">Login Page</a></li>
				</button>
				<button class="button-header">
					<li class="link"><a href="/auth/register">Register Page</a></li>
				</button>
				
				<% if (page !== "register" && page !== "login") { %>
					<li class="link">
						<form action="/auth/logout" method="post">
							<button class="button-header-logout" type="submit">Logout</button>
						</form>
					</li>
					<% } %>
				</ul>
			</nav>
		</div>
		<div id="header-user">
			<% if (page === "index") { %>
			<p>User: <%= email %></p>
			<% } %>
		</div>
</header>
